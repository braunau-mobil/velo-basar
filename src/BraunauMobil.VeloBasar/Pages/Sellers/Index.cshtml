@page
@model BraunauMobil.VeloBasar.Pages.Sellers.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].Street)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].ZIP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].Country.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].IBAN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].BIC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].BankAccountHolder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller[0].Token)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Seller) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Street)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ZIP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Country.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IBAN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BIC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BankAccountHolder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Token)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
