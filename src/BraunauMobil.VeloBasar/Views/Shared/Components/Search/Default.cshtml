@model SearchModel

<div class="input-group">
    <input type="text" class="form-control" autofocus name="@nameof(Model.SearchString)" value="@Model.SearchString" />
    <button class="btn btn-outline-primary">@Localizer[VeloTexts.Search]</button>
    <a class="btn btn-outline-secondary" href="@Model.ResetUrl">@Localizer[VeloTexts.Reset]</a>
</div>
